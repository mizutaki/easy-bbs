<% @store.each{|content|  %>
<hr>
<table>
	<tr>
		[<%= content.get_contribution_number %>]
		<th>投稿時刻：</th>
		<td>
			<%=content.get_write_date %>
		</td>
	</tr>
	<tr>
		<th>お名前：</th>
		<td>
			<%=content.get_name %>
		</td>
	</tr>
	<tr>
		<th>メッセージ：</th>
		<td>
			<%=content.get_message %>
		</td>
	</tr>
</table>
<hr>
<% } %>
<% if @current_page != 1 %>
	<a href="/prev">前のページ</a>
<% end %>
<% if @store.length == 10  %>
	<a href="/next/1">次のページ</a>
<% end %>

